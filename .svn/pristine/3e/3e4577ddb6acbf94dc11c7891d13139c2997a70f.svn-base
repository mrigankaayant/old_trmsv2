<body>
	<div>
		<div class="ui-ribbon">
			<ui:insert name="header">
				<h:form>
					<p:tabMenu activeIndex="#{param.i}" style="border:none;">

						<p:menuitem value="Recruitment" outcome="/recruitment/recruitment.xhtml"
							disabled="#{!loginBean.hasRole('ROLE_RECRUITER')}">
							<f:param name="i" value="0" />
						</p:menuitem>

						<p:menuitem activeIndex="#{param.i}" value="Admin" outcome="/admin/admin.xhtml"
							disabled="#{!loginBean.hasRole('ROLE_ADMIN')}">
							<f:param name="i" value="1" />
						</p:menuitem>

						<p:menuitem activeIndex="#{param.i}" value="Self Service" outcome="/selfService/selfservice.xhtml">
							<f:param name="i" value="2" />
						</p:menuitem>

						<p:menuitem activeIndex="#{param.i}" value="Account" outcome="/account/account.xhtml"
							disabled="#{!loginBean.hasRole('ROLE_ACCOUNT')}">
							<f:param name="i" value="3" />
						</p:menuitem>

						<p:menuitem activeIndex="#{param.i}" value="Report" outcome="/report/report.xhtml"
							disabled="#{ !loginBean.hasRole('ROLE_ACCOUNT')  }">
							<f:param name="i" value="4" />
						</p:menuitem>

						<p:menuitem activeIndex="#{param.i}" value="Training" outcome="/trainer/trainer.xhtml"
							disabled="#{ !loginBean.hasRole('ROLE_HR')}">
							<f:param name="i" value="5" />
						</p:menuitem>

						<p:menuitem activeIndex="#{param.i}" value="HR" outcome="/hr/hr.xhtml"
							disabled="#{ !loginBean.hasRole('ROLE_HR')}">
							<f:param name="i" value="6" />
						</p:menuitem>

						<p:menuitem activeIndex="#{param.i}" value="Sales" outcome="/sales/sales.xhtml"
							disabled="#{ !loginBean.hasRole('ROLE_SALES')}">
							<f:param name="i" value="7" />
						</p:menuitem>

					</p:tabMenu>
				</h:form>
			</ui:insert>
			<ui:insert name="submenu">Put default submenu here, if any.</ui:insert>
		</div>

		<div style="margin-top: 116px;">
